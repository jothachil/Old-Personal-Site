<div id="full-container" canvas="container">
    <%- partial("partials/_nav") %>
    <div class="ticket-container">
        <div class="ticket-landing-container">
            <div class="container">

                <div class="row">
                    <div class="col col--6-of-12 col--m-2-of-4 col--s-2-of-2 ticket-landing-text">
                        <h1>Ticket New Redesign</h1>
                    </div>
                    <div class="col col--6-of-12 col--m-2-of-4 col--s-2-of-2">
                        <img class="fadein" src="img/ticket.png" alt="">
                    </div>

                </div>
            </div>
        </div>
        <div class="down">
            <i class="fa fa-chevron-down" aria-hidden="true"></i>

        </div>
